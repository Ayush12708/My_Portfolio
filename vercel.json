{
    "builds": [
        {
            "src": "myportfolio/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/portfolio/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/portfolio/projects/$1"
        },
        {
            "src": "/(.*)",
            "dest": "myportfolio/wsgi.py"
        }
    ]
}